#!/bin/bash
clish -c 'set interface LAN1 state on'
clish -c 'set interface LAN1 ipv4-address 192.168.1.1 subnet-mask 255.255.255.0'
clish -c 'set interface LAN2 state on'
clish -c 'set interface LAN2 ipv4-address 100.64.64.1 subnet-mask 255.255.255.248'
clish -c 'add internet-connection interface LAN3 type static ipv4-address 169.254.10.2 subnet-mask 255.255.255.248 default-gw 169.254.10.3'
clish -c 'add internet-connection interface LAN4 type static ipv4-address 169.254.11.2 subnet-mask 255.255.255.248 default-gw 169.254.11.3'
clish -c 'add interface LAN1 vlan 10 ipv4-address 192.168.10.1 subnet-mask 255.255.255.0'
clish -c 'add interface LAN1 vlan 11 ipv4-address 192.168.11.1 subnet-mask 255.255.255.0'
clish -c 'set interface LAN10 state on'
clish -c 'set interface LAN10 ipv4-address 169.254.9.1 subnet-mask 255.255.255.252'
clish -c 'set device-details hostname Singapore-FW02'
clish -c 'set security-management mode centrally-managed'
clish -c 'set security-management local-override-mgmt-addr true mgmt-address 198.18.251.11 send-logs-to local-override-mgmt-addr'
clish -c 'set sic_init password aryaka123'
clish -c 'add static-route destination 198.18.251.11/32 nexthop gateway ipv4-address 100.64.64.2'
clish -c 'connect security-management mgmt-addr 198.18.251.11 use-one-time-password true local-override-mgmt-addr true send-logs-to local-override-mgmt-addr'
clish -c 'set dns primary ipv4-address 8.8.8.8 secondary ipv4-address 4.2.2.2'
clish -c 'test'
clish -c 'add static-route destination host nexthop gateway ipv4-address 1.2.3.4'
clish -c 'add static-route destination 10.10.10.1/32 nexthop gateway ipv4-address 1.2.3.4'
clish -c 'add static-route destination 10.10.10.2/32 nexthop gateway ipv4-address 1.2.3.4'
clish -c 'add static-route destination 10.10.10.3/32 nexthop gateway ipv4-address 1.2.3.4'
clish -c 'add static-route destination 10.10.20.0/24 nexthop gateway ipv4-address 1.2.3.4'
clish -c 'add static-route destination 10.10.1.0/24 nexthop gateway ipv4-address 1.2.3.4'
